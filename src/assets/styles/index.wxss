}

.shoper_best_goods .shoperBanner {
  position: relative;
}

.shoper_best_goods .shoperBanner:before {
  position: absolute;
  content: '';
  width: 0;
  height: 0;
  border-style: solid;
  border-width: 10px 8px;
  border-color: transparent transparent #f1f1f1;
  bottom: 0;
  right: 12%;
}

.sortImg .chosenAvater image{
