@import "./weSwiper.wxss";

page {
  font-family: '微软雅黑';
  color: #333;
  font-size: 30rpx;
}

/*头部导航栏*/
.index-title {
  background: url(http://wxassets.xf66.com/index_bg.jpg) no-repeat center top;
  background-size: 100%;
  position: relative;
  color: #fff;
  z-index: 11;
  width: 100%;
}

.index-title .unshop-name {
  height: 44px;
  line-height: 44px;
  padding-top: 0;
  text-align: center;
  overflow: hidden;
}

.index-title .shop-name {
  text-align: center;
  overflow: hidden;
  color: #fff;
  padding-top: 11.5rpx;
  position: relative;
  /*height: 84px;*/
}

.index-title .shop-name .contact_img {
  position: absolute;
  left: 27rpx;
  top: 10rpx;
}

.contact_img text, .search-icon text {
  font-size: 20rpx;
  color: #fff;
  position: relative;
  top: -14rpx;
}

.contact_img image, .search-icon image {
  width: 47rpx;
  height: 47rpx;
  display: block;
}

.shop-name .contact_img .shoper_contact {
  font-size: 11px;
  border-radius: 10px;
  background: #fff;
  color: #ef0021;
  padding: 0px 4px;
  display: inline-block;
  float: left;
  margin-top: 7px;
  margin-left: 7px;
}

.index-title .shop-detail {
  color: #fff;
  text-align: center;
}

.index-title .shop-detail .user-img {
  float: left;
  height: 36px;
}

@media screen and (min-width:400px) and (max-width: 420px) {
  .index-title .shop-name .user-img {
    padding-bottom: 4.5rpx;
  }
}
.user-img{
  width:78rpx; height: 78rpx; margin:0 auto; border-radius:39rpx; overflow:hidden;
}
.index-title .user-img image {
  border-radius: 37rpx;
  border: 1px solid #eee;
  background-size: cover;
  background-repeat: no-repeat;
  background-position: center;
  vertical-align: middle;
  width: 74rpx;
  height: 74rpx;
  background-color: #fff;
  overflow:hidden;
}

.index-title .shop-detail .shop-info {
  position: relative;
  display: flex;
  align-items: center;
  flex-direction: column;
}

.index-title .search-icon {
  position: absolute;
  right: 15px;
  top: 11.5rpx;
  color: #777;
}

.index-title .search-icon image {
  width: 47rpx;
  height: 47rpx;
  display: block;
}

.index-title .search-icon .search_css {
  color: #fff;
  font-size: 18px;
}

.shop-info .nhh-store_name {
  font-size: 28rpx;
}

.index-title .shop-detail .shop-info .info {
  font-size: 20rpx;
  display: block;
  height: 18px;
  overflow: hidden;
  padding: 0 20px;
  text-overflow: ellipsis;
  white-space: nowrap;
  opacity: 0.5;
  margin-top: -5rpx;
}

/*轮播图*/
.slide-img {
  width: 100%;
  height: 212px;
}

/*间隔线*/
.separate {
  height: 10rpx;
  background-color: #f8f8f8;
}

/*导航栏*/
.nav-slide {
  white-space: nowrap;
  border-top: 2rpx solid #eee;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  background: #f6f6f6;
  position: relative;
}

.nav-slide view {
  width: 216rpx;
  display: inline-block;
  overflow: hidden;
}

.nav-slide view:last-child {
  margin-right: 60rpx;
}

.nav-slide .active {
  border-bottom: 4rpx solid #ef0021;
  font-weight: bold;
  height: 76rpx;
}

