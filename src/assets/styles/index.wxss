}

/*拍卖部分样式*/
.auction {
  background-color:#FEFEFE;
}
.auction .we-wrapper{
  width: 100%;
  height: 100%;
  padding: 24rpx 0 24rpx 75rpx;
  box-sizing: border-box;
}
.auction .we-slide{
  flex-direction: column;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 550rpx;
  margin: 25rpx;
  box-sizing: border-box;
  color: #fff;
  font-size: 2rem;
}
.auction .auctionTitle{
  border-bottom: 1px solid #e5e8f1;
}
.auction .goods .goodsImg{
  position:relative;
  display:inline-block;
  width:100%;
}
.auction .goods .goodsImg image{
  width:100%;
  display: block;
}
.auction .goods .goodsImg .tips{
  position:absolute;
  top:10rpx;
  left:10rpx;
  display:inline-block;
  padding-right:6rpx;
  height:34rpx;
  line-height:34rpx;
  background-color:#BFB7B7;
  border-radius:3px;
  font-family:'PingFang-SC_Bold';
  font-size:20rpx;color:#fff;
}
.auction .goods .goodsImg .tips .dot {
  width:10rpx;
  height:10rpx;
  border-radius:50%;
  background-color:#000;
  display:inline-block;
  margin:0 6rpx;
}
.auction .goods .goodsInfo{
  width: 100%;
  box-sizing: border-box;
  background-color:#fff;
  padding:36rpx 30rpx 28rpx;
}
.auction .goodsInfo .title{
  font-size:24rpx;
  font-family:'PingFang-SC_Bold';
  overflow:hidden;
}
.auction .goodsInfo .number{
  font-size:24rpx;
  font-family:'PingFang-SC_Medium';
}
.auction .auctionData{
  padding:0 15px;
  font-size:24rpx;
  font-family:'PingFang-SC_Bold';
  border-bottom:1px solid #eee;
  padding-bottom:20rpx;
}
.auction .seeMore {
  display:block;
  height:84rpx;
  line-height:84rpx;
  font-family:'PingFang-SC_Medium';
  text-align:center;
}
.auction .goods.active {
  box-shadow: 0 10rpx 14rpx 2px #ccc;
}
.fixTop{
  position: fixed;
  top:0;
  z-index:8;
  max-width:640px;
  min-width:320px;
  width:100%;
  margin: 0 auto
}

